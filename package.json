{
  "name": "api-mock-generator",
  "version": "1.0.0",
  "description": "Production-ready mock server generator from OpenAPI/Swagger specifications with 150+ dynamic templates",
  "private": false,
  "bin": {
    "api-mock": "./packages/cli/dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Utkautka1/API-Mock-Server-Generator.git"
  },
  "bugs": {
    "url": "https://github.com/Utkautka1/API-Mock-Server-Generator/issues"
  },
  "homepage": "https://github.com/Utkautka1/API-Mock-Server-Generator#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "packages/*/dist/**/*",
    "packages/*/package.json",
    "packages/ui/dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces --if-present",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "clean": "npm run clean --workspaces",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openapi",
    "swagger",
    "mock",
    "api",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  }
}
